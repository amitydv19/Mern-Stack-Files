
// function definition
function sayMyName() {
    console.log ("Amit Ray")
}
// Error when we run before fn call

//function use - function call
sayMyName();
// Output: Amit Ray

// Function declaration
function getMultiplication(a,b){
    return a*b;
}

console.log(getMultiplication(20,10));

//Output: 200

// Function Expression

const greet = function(name){
    return `Hello, ${name}!`;

}
console.log(greet ("John"));

//Ouput: Hello, John!

console.log(greet("John"));
